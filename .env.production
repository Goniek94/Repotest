# Adres API na produkcji
REACT_APP_API_URL=https://testauto.cba.pl/api
# Wyłączenie map źródłowych dla zwiększenia wydajności
GENERATE_SOURCEMAP=false
# Tryb produkcyjny
NODE_ENV=production
# Cache
REACT_APP_CACHE_TTL=3600000
# Czas życia tokenu
REACT_APP_TOKEN_EXPIRY=86400000
# Wykrywanie błędów
REACT_APP_ERROR_LOGGING=true
# Liczba prób ponownego połączenia
REACT_APP_MAX_RETRIES=3
# Throttling zapytań
REACT_APP_THROTTLE_REQUESTS=true
# Wersja API
REACT_APP_API_VERSION=v1
# Wyłączenie logowania debugowania
REACT_APP_DISABLE_CONSOLE_IN_PRODUCTION=true
